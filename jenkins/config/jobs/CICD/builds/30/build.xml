    <script>def frontendImage
def backendImage
pipeline {
    agent any
    stages {
        stage(&apos;Build Test&apos;) { 
            steps {
                echo &apos;hello World!!&apos;
            }
        }

        stage(&apos;Build Images&apos;){
            parallel {
                stage(&apos;Build Frontend Image&apos;) { 
                    steps {
                        script {
                            docker.withRegistry(&apos;&apos;, &apos;Dockerhub&apos;) {
                                frontendImage = docker.build(&quot;omarlaz/frontend:latest&quot;, &quot;./Frontend&quot;)
                            }
                        } 
                    }

                    post {
                        success {
                            // echo &apos;Frontend image was built successfully!!&apos;
                            emailext body: &apos;Test Message&apos;,
                                    subject: &apos;Test Subject&apos;,
                                    to: &apos;jenkinsproject51@gmail.com&apos;
                        }
                        failure {
                            echo &apos;failed&apos;
                            // emailext body: &apos;A Test EMail&apos;, recipientProviders: [[$class: &apos;DevelopersRecipientProvider&apos;], [$class: &apos;RequesterRecipientProvider&apos;]], subject: &apos;Test&apos;
                        }
                    }
                }

                stage(&apos;Build Backend Image&apos;) { 
                    steps {
                        script {
                            docker.withRegistry(&apos;&apos;, &apos;Dockerhub&apos;) {
                                backendImage = docker.build(&quot;omarlaz/backend:latest&quot;, &quot;./Backend&quot;)
                            }
                        } 
                    }
                }
            }
        }

        stage(&apos;Push Images&apos;){
            parallel {
                stage(&apos;Push Frontend Image&apos;) { 
                    steps {
                        script {
                            docker.withRegistry(&apos;&apos;, &apos;Dockerhub&apos;) {
                                frontendImage.push()
                            }
                        } 
                    }
                }

                stage(&apos;Push Backend Image&apos;) { 
                    steps {
                        script {
                            docker.withRegistry(&apos;&apos;, &apos;Dockerhub&apos;) {
                                backendImage.push()
                            }
                        } 
                    }
                } 
            }
        }
    }    
}</script>
